{
  "manifest_version": 3,
  "name": "Thống Kê Lượt Truy Cập Web",
  "version": "1.0.0",
  "description": "Thống kê số lượt truy cập và lịch sử các lần truy cập trang web",
  "permissions": [
    "tabs",
    "history",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

